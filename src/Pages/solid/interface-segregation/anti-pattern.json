"import React,{FC} from 'react';\n\nconst resource:IResource = {\n\tname: 'Name',\n\tage: '20',\n\treactLevel: 10,\n\tjsLevel: 9,\n};\n\ninterface IResource {\n\tname: string;\n\tage:string;\n\treactLevel: number;\n\tjsLevel: number\n}\n\nconst GeneralInfo:FC<IResource> = (props:IResource) => {\n\tconst {name, age} = props;\n\treturn (\n\t\t<>\n\t\t\t<p>name:{name}</p>\n\t\t\t<p>age:{age}</p>\n\t\t</>\n\t);\n};\n\nconst SkillLevelInfo:FC<IResource> = (props:IResource) => {\n\tconst {reactLevel, jsLevel} = props;\n\treturn (\n\t\t<>\n\t\t\t<p>reactLevel:{reactLevel}</p>\n\t\t\t<p>jsLevel:{jsLevel}</p>\n\t\t</>\n\t);\n};\n"
